{
  "name": "leeksdao_frontend",
  "version": "0.0.0",
  "private": true,
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.tsx": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:css": "stylelint 'src/**/*.css'",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.12.0",
    "@wagmi/core": "^0.9.2-cjs",
    "@web3modal/ethereum": "^2.0.0",
    "@web3modal/html": "^2.0.0",
    "css.gg": "^2.0.0",
    "ethers": "^5.7.2",
    "pinia": "^2.0.26",
    "vue": "^3.2.45",
    "vue-i18n": "9",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.8",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "cypress": "^11.2.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^9.3.0",
    "exact": "^0.8.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-order": "^6.0.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.4",
    "vite-plugin-dynamic-import": "^1.2.7",
    "vite-plugin-stylelint": "^4.2.0",
    "vitest": "^0.25.3",
    "vue-eslint-parser": "^7.11.0",
    "vue-tsc": "^1.0.9"
  }
}
